<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".MainMenu"
    tools:showIn="@layout/activity_main_menu">


    <!--<TextView-->
        <!--android:id="@+id/textView"-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:text="TextView" />-->

    <Button
        android:id="@+id/button4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_row="1"
        android:layout_column="1"
        android:text="START TOUR"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="170dp"
        tools:text="Start Tour" />

    <Button
        android:id="@+id/YGbutton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Your Groups" />

    <Button
        android:id="@+id/JEGbutton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Join Existing Group" />

    <Button
        android:id="@+id/button5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="BROWSE TOURS"
        tools:text="Browse Tours" />

    <Button
        android:id="@+id/button6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="SETTINGS"
        tools:text="Settings" />

    <Button
        android:id="@+id/button7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="LOG OUT"
        tools:text="Log Out" />
</LinearLayout>